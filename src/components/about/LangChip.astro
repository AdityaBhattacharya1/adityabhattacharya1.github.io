---
type Props = {
	name: string
	key: number
}
const { name } = Astro.props
---

<style>
	.lang-chip {
		border: 3px solid #44ab96;
		border-radius: 25px;
		padding: 5px 10px;
		margin: 5px;
		background-color: #44ab9734;
		display: flex;
		align-items: center;
	}
</style>
<div class="lang-chip">
	{name}
</div>
